{
   "cli": {
      "version": ">= 15.0.11",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "autoIncrement": true
      },
      "preview": {
         "distribution": "internal",
         "autoIncrement": true
      },
      "production": {
         "autoIncrement": true,
         "android": {
            "buildType": "app-bundle",
            "gradleCommand": ":app:bundleRelease",
            "credentialsSource": "remote",
            "gradleProperties": {
               "android.targetSdkVersion": "35",
               "android.compileSdkVersion": "35"
            }
         }
      }
   },
   "submit": {
      "production": {}
   }
}
